{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/settings",
      "dest": "/static/settings.html"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "static": {
    "directory": "static"
  }
}
